<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/LinearLabelFlaps"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_flaps"
            android:textStyle="bold"
            android:gravity="center"
            android:padding="14dp"
            android:textSize="12pt"/>
        <TextView
            android:id="@+id/LinearLabelFlightTakeoff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_flight_takeoff"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearLabelInpFlightTakeoff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearLabelTakeoffLanding"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_takeoff_landing"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearLabelInpTakeoffLangding"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="text"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearLabelFlightLanding"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_flight_landing"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearLabelInpFlightLanding"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearLabelLowPrc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_low_prc"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearLabelInpLowPrc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearLabelN18511"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_brake"
            android:textStyle="bold"
            android:gravity="center"
            android:padding="14dp"
            android:textSize="12pt"/>
        <TextView
            android:id="@+id/LinearLabelRelease"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_release"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearLabelInpRelease"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearLabelCleaning"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_cleaning"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearLabelInpCleaning"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearLabelLowPrc2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_low_prc"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearLabelInpLowPrc2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearLabelSpace"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearLabelTmc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_tmc"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearLabelInpTmc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearLabelVGenerator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_v_generator"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearLabelInpVGenerator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="5">
            <Button
                android:id="@+id/LinearButtonBackClosingN85"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:gravity="center"
                android:layout_gravity="start"
                android:text="@string/button_back"
                android:layout_weight="1"
                />
            <TextView
                android:id="@+id/LinearTextViewCenterClosingN85"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="0sp"
                android:gravity="center"
                android:layout_weight="3"/>
            <Button
                android:id="@+id/LinearButtonNextN85"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:gravity="center"
                android:layout_gravity="end"
                android:text="@string/button_next"
                android:layout_weight="1"
                />
        </LinearLayout>
    </LinearLayout>
</ScrollView>