<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/LinearN1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_n1"
            android:padding="5dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpN1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="numberDecimal"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center" />
        <TextView
            android:id="@+id/LinearN2_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_n2_4"
            android:padding="5dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpN2_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="numberDecimal"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center" />
        <TextView
            android:id="@+id/LinearTRC"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_trc"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpTRC"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearPm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_pm"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpPm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="numberDecimal"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearTmC"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_tmc"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpTmC"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearPt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_pt"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpPt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearEngineSqrt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_engine_sqrt"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpEngineSqrt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearVGenerator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_v_generator"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpVGenerator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearVoltageRegulator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_voltage_regulator"
            android:textStyle="bold"
            android:gravity="center"
            android:padding="14dp"
            android:textSize="12pt"/>
        <TextView
            android:id="@+id/LinearLabelAzsAgdGmk"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_azs_agd_gmk"
            android:textStyle="italic"
            android:gravity="center"
            android:textSize="12pt"/>
        <TextView
            android:id="@+id/LinearTurnOn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_turn_on"
            android:textSize="10pt"
            android:gravity="center"/>
        <Spinner
            android:id="@+id/RelativeSpinnerInpKrTank"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center" />
        <TextView
            android:id="@+id/LinearLabelSpace"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearPCabin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_p_cabin"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpPCabin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="numberDecimal"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>

        <TextView
            android:id="@+id/LinearPVozdKrTank"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_pvozd_kr_tank"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpPVozdKrTank"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <TextView
            android:id="@+id/LinearPtPreembossed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/label_pt_preembossed"
            android:textSize="10pt"
            android:gravity="center"/>
        <EditText
            android:id="@+id/LinearInpPtPreembossed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/enter"
            android:inputType="number"
            android:paddingBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:textSize="10pt"
            android:gravity="center"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <Button
                android:id="@+id/LinearButtonNextClosingKVD3Max"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:gravity="center"
                android:layout_gravity="end"
                android:text="@string/button_next"
                />
        </LinearLayout>
    </LinearLayout>
</ScrollView>